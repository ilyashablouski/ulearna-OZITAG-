{{!Modules}}
{{> layout/head}}

<section class="uikit-module">
  <header class="uikit-module__header">
    <span class="uikit-module__title">Page Header</span>
    <span class="uikit-module__path">modules/page-header</span>
  </header>
  <div class="uikit-module__content">
    {{> modules/page-header/signup }}
    {{> modules/page-header/signup-success }}
  </div>
</section>

<section class="uikit-module">
  <header class="uikit-module__header">
    <span class="uikit-module__title">Carousel</span>
    <span class="uikit-module__path">modules/carousel</span>
  </header>
  <div class="uikit-module__content">
    {{> modules/carousel/home-featured }}
    {{> modules/carousel/home-plus }}
  </div>
</section>


{{> layout/bottom}}


